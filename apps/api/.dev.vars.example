# Session secret for signing cookies (generate a random string)
SESSION_SIGNING_SECRET=change-me-to-random-string

# Gemini API key from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=

# R2 S3-compatible API credentials
# Create these in Cloudflare Dashboard → R2 → Manage R2 API Tokens
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=

# Google OAuth (optional)
GOOGLE_CLIENT_SECRET=

# Turnstile secret key (optional, set TURNSTILE_BYPASS=true to skip)
TURNSTILE_SECRET_KEY=

# Set to true for local dev to bypass Turnstile
TURNSTILE_BYPASS=true
